@model Filmy.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<h2 class="text-success">@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Type</li>
    <li>Release Date:@Model.ReleaseDate</li>
    <li>Date Added:@Model.DateAdded </li>
    <li>Number In Stock:@Model.NumberInStock </li>

</ul>

